# SpaceTraders API Configuration
# Copy this file to .env and fill in your actual values

# Your SpaceTraders agent token (if you already have an agent)
AGENT_TOKEN=your_agent_token_here

# Your SpaceTraders account token (if you need to register a new agent)
ACCOUNT_TOKEN=your_account_token_here

# API Base URL (default: https://api.spacetraders.io)
SPACETRADERS_API_URL=https://api.spacetraders.io